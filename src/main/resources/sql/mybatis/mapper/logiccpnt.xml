<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.develope.plto.mapper">

	<select id="selectLogicCpnt" parameterType="int" resultType="com.develope.plto.domain.LogicCpnt">
		select * from tb_logic_cpnt where FD_CATEGORY=#{FD_CATEGORY}
	</select>
	<select id="selectAllLogicCpnt" resultType="com.develope.plto.domain.LogicCpnt">
		select * from tb_logic_cpnt
	</select>
</mapper>